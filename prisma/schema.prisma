datasource db {
    provider = "sqlite"
    url      = "file:dev.db"
}

generator client {
    provider = "prisma-client-js"
}

// Comic model
model Comic {
    id          Int    @id @default(autoincrement())
    title       String
    description String
    hover       String
    source      String
    height      Int    @default(480)
    width       Int    @default(768)
}
